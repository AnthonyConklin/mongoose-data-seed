{
  "name": "mongoose-data-seed",
  "version": "1.0.0",
  "description": "Seed mongodb with data using mongoose models",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "babel src/ --out-dir dist/ --ignore 'src/**/*.spec.js'",
    "test": "cross-env NODE_ENV=test node_modules/.bin/nyc --no-cache node --harmony-proxies node_modules/.bin/ava",
    "test:watch": "npm run test -- --watch",
    "test:check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "coveralls": "nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "lint": "xo",
    "slate": "rimraf node_modules && npm install",
    "prepublish": "npm run build"
  },
  "bin": {
    "md-seed": "./dist/bin/index.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "ava": {
    "files": [
      "src/lib/**/*.spec.js"
    ],
    "source": [
      "src/lib/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "./src/lib/utils/setup-test-env.js"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "src/lib/config.js",
      "src/lib/utils/setup-test-env.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "xo": {
    "extends": "xo-space",
    "ignores": [
      "templates/**/*",
      "examples/**/*"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharvit/mongoose-data-seed.git"
  },
  "keywords": [
    "mongoose",
    "seed",
    "data",
    "mongodb",
    "seeder",
    "mongoosejs"
  ],
  "author": "Avi Sharvit <sharvita@gmail.com> (https://sharvit.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharvit/mongoose-data-seed/issues"
  },
  "homepage": "https://github.com/sharvit/mongoose-data-seed#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chalk": "^1.1.3",
    "clui": "^0.3.1",
    "command-line-args": "^4.0.2",
    "command-line-commands": "^2.0.0",
    "command-line-usage": "^4.0.0",
    "find-root": "^1.0.0",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "log-symbols": "^1.0.2",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "rxjs": "^5.3.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-xo-space": "^0.16.0",
    "nyc": "^10.2.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "xo": "^0.18.1"
  }
}
